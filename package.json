{
  "name": "apiautomator",
  "version": "1.0.0",
  "description": "Lightweight Node Application for the API Automation Testing ",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/rest*Test.js --reporter mocha-simple-html-reporter --reporter-options output=reports/html/restTestReport.html",
    "smoke": "mocha test/smoke/smokeTest.js --reporter mochawesome --reporter-options reportDir=reports/html/,reportFilename=smokeTestReport.html",
    "regression": "mocha test/regression/regressionTest.js --reporter mochawesome --reporter-options reportDir=reports/html/,reportFilename=regressionTestReport.html",
    "endtoend": "mocha test/endtoend/endtoendTest.js --reporter mochawesome --reporter-options reportDir=reports/html/,reportFilename=endtoendTestReport.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravivamsi/apiautomator.git"
  },
  "keywords": [
    "Node",
    "API",
    "Rest",
    "SOAP",
    "Automation",
    "Schema",
    "Assertions",
    "StatusCodes",
    "BDD",
    "Response",
    "Body"
  ],
  "author": "Vamsi Ravi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ravivamsi/apiautomator/issues"
  },
  "homepage": "https://github.com/ravivamsi/apiautomator#readme",
  "dependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-json-schema": "^1.5.0",
    "chakram": "^1.5.0",
    "fs": "0.0.1-security",
    "handlebars": "^4.0.11",
    "mocha": "^5.2.0",
    "mocha-simple-html-reporter": "^1.1.0",
    "mochawesome": "^3.0.2",
    "request": "^2.87.0",
    "should": "^13.2.1",
    "soap": "^0.24.0",
    "sys": "0.0.1",
    "xsd-schema-validator": "^0.6.0"
  }
}
